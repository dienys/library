<header class="navbar navbar-fixed-top navbar-inverse">
    <div class="container">
        <nav>
            <ul class="nav navbar-nav navbar-left">
                
            </ul>
            <ul class="nav navbar-nav navbar-right">
              <% if current_user %>
                <li class= "username">Logged in as <%= current_user.username %></li>
